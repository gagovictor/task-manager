    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll --coverage --transformIgnorePatterns \"node_modules/(?!msw)/\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios)/)",
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@types/redux-mock-store": "^1.0.6",
    "@types/redux-thunk": "^2.1.0",
    "babel-jest": "^29.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "msw": "^2.4.0",
    "prettier": "^3.3.3",
    "redux-mock-store": "^1.5.4"
  }
}
